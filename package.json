{
  "name": "reservation-api",
  "version": "0.0.1",
  "description": "Reservation Api",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/**/*.spec.js",
    "coverage": "nyc --reporter=html --include='app/**/*.js' --exclude='app/models/**/*.js' --reporter=text mocha tests/**/*.spec.js",
    "start": "nodemon node index.js",
    "init": "pm2-runtime index.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "nginx": "docker-compose build & docker-compose up -d & npm run migrate & npm run seed",
    "local": "docker-compose up -d db & npm run migrate & npm run seed & npm run start"
  },
  "keywords": [
    "API",
    "Reservation"
  ],
  "author": "Anderson Oliveira Martins",
  "devDependencies": {
    "mocha": "8.2.0",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "proxyquire": "2.1.0",
    "should": "13.2.3",
    "supertest": "4.0.2"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "bunyan": "1.8.12",
    "encode32": "^1.1.0",
    "express": "4.17.1",
    "knex": "^0.21.12",
    "pg": "^8.5.1",
    "swagger-ui-express": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "standard": {
    "globals": [
      "describe",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "it",
      "assert"
    ]
  }
}
